<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="darius.partene.mycontacts">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyContacts"
        android:allowBackup="true"
        android:icon="@drawable/ic_account_circle_blue_24dp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.ActivityContacts">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.ActivityContactDetails">
        </activity>
        <activity android:name=".activities.BaseActivity">
        </activity>
        <activity android:name=".activities.ToolbarActivity">
        </activity>
    </application>

</manifest>